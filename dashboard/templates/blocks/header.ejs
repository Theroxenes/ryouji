	<script>
    $(window).on('load', function() {
      $('#loading').hide();
      $('.pageContent').show();
      
      function warnConsole () { // Bit useless for the type of site that this is but added it in anyway
        console.clear();
        console.log('%c' + 'Que milaque o(a) senhor(a) por aqui, Não gostaria de entrar para tomar uma xícara de café?', 'font-size: 18px; font-weight: bold; color: red;');
        console.log('%c' + 'Bem... Eu não tenho muito à mostrar aqui. :/ ', 'font-size: 18px;');
      }
      warnConsole();
      window.onresize = function(){ // Window resize triggers warnConsole function. False positives shouldn't be a problem as the user will only see this if they open the console
        if ((window.outerHeight - window.innerHeight) > 100) {
            warnConsole();
        }
      }
    });
		function setCookie(cname, cvalue, exdays) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays*24*60*60*1000));
			var expires = "expires="+ d.toUTCString();
			document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
		}
		function getCookie(cname) {
				var name = cname + "=";
				var decodedCookie = decodeURIComponent(document.cookie);
				var ca = decodedCookie.split(';');
				for(var i = 0; i <ca.length; i++) {
						var c = ca[i];
						while (c.charAt(0) == ' ') {
								c = c.substring(1);
						}
						if (c.indexOf(name) == 0) {
								return c.substring(name.length, c.length);
						}
				}
				return "";
		}
	</script>
